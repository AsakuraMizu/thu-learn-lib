{
  "name": "thu-learn-lib",
  "version": "3.1.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "type": "module",
  "module": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "types": "./lib/index.d.ts"
    },
    "./lib/types": {
      "import": "./lib/types.js",
      "types": "./lib/types.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "vite",
    "format": "prettier --write \"{src,test,demo}/**/*.{ts,html,json}\" \"*.{ts,md,json}\"",
    "lint": "eslint --ext .ts 'src/**'",
    "test": "vitest run"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/node": "^20.5.9",
    "@types/tough-cookie": "^4.0.2",
    "@types/webextension-polyfill": "^0.10.2",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "domhandler": "^5.0.3",
    "dotenv": "^16.3.1",
    "eslint": "^8.48.0",
    "jest": "^29.6.4",
    "prettier": "^3.0.3",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vite": "^4.4.9",
    "vite-plugin-web-extension": "^3.2.0",
    "vitest": "^0.34.3",
    "webextension-polyfill": "^0.10.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "entities": "^4.5.0",
    "fetch-cookie": "^2.1.0",
    "js-base64": "^3.7.5",
    "node-fetch-native": "^1.4.0",
    "tough-cookie": "^4.1.3"
  }
}
