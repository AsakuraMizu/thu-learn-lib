{
  "name": "thu-learn-lib",
  "version": "3.0.4",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "type": "module",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc && sed -i -E \"s/from '(\\.\\/.*)'/from '\\1.js'/g\" lib/*.js",
    "build-dist": "webpack --config webpack.config.js",
    "watch-dist": "webpack -w --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"demo/**/*.ts\" \"./*.js\"",
    "lint": "eslint --ext .ts 'src/**'",
    "test": "jest --config jest.config.json"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com> (https://harrychen.xyz)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/firefox-webext-browser": "^111.0.1",
    "@types/jest": "^29.5.0",
    "@types/node": "^20.3.1",
    "@types/tough-cookie": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "copy-webpack-plugin": "^11.0.0",
    "domhandler": "^5.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.38.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.5.0",
    "node-polyfill-webpack-plugin": "^2.0.1",
    "prettier": "^2.8.7",
    "remove-files-webpack-plugin": "^1.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.78.0",
    "webpack-cli": "^5.0.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.12",
    "entities": "^4.4.0",
    "es6-denodeify": "^0.1.5",
    "fetch-cookie": "^2.1.0",
    "js-base64": "^3.7.5",
    "node-fetch-native": "^1.2.0",
    "tough-cookie": "^4.1.3"
  }
}
