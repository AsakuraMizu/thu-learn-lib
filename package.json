{
  "name": "thu-learn-lib",
  "version": "2.4.0",
  "description": "a JavaScript library to provide a program-friendly interface to Web Learning (2018) of Tsinghua University",
  "main": "lib/index.js",
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "scripts": {
    "build": "tsc",
    "build-dist": "webpack --config webpack.config.js",
    "watch-dist": "webpack -w --config webpack.config.js",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"demo/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest"
  },
  "keywords": [
    "tsinghua",
    "learn2018"
  ],
  "author": "Harry Chen <harry-chen@outlook.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Harry-Chen/thu-learn-lib.git"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.15",
    "@types/firefox-webext-browser": "^82.0.0",
    "@types/jest": "^26.0.9",
    "@types/js-base64": "^3.0.0",
    "dotenv": "^8.2.0",
    "html-webpack-plugin": "^5.0.0-beta",
    "jest": "^26.3.0",
    "node-polyfill-webpack-plugin": "^1.0.2",
    "prettier": "^2.0.5",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.2",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "^4.0.2",
    "webpack": "^5.15.0",
    "webpack-cli": "^4.3.1"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.2",
    "cross-fetch": "^3.0.1",
    "entities": "^2.0.0",
    "es6-denodeify": "^0.1.5",
    "form-data": "^3.0.0",
    "js-base64": "^3.4.5",
    "real-isomorphic-fetch": "^3.0.0"
  }
}
