	"minimum_chrome_version" : "51",
	"manifest_version" : 2,
	"description":"清华大学网络学堂2018爬虫测试",
	"content_scripts":[
	],
	"background" : {
        "scripts": [
			"browser-polyfill.min.js",
			"index.js"
